<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lowLayout"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    app:layout_constraintBottom_toTopOf="@id/footer"
    app:layout_constraintTop_toBottomOf="@id/header"
    app:layout_constraintVertical_weight="5">


    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/innerLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp">

        <include
            android:id="@+id/serialLayout"
            layout="@layout/form_serial"/>


        <TextView
            android:id="@+id/tvGSsoftware1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsTextsoftware1"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"

            app:layout_constraintTop_toBottomOf="@id/serialLayout" />

        <EditText
            android:id="@+id/etGSsoftware1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/gsSoftwareVer"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware1"
            app:layout_constraintLeft_toRightOf="@id/tvGSsoftware1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware1" />

        <TextView
            android:id="@+id/tvGSsoftware2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsTextsoftware2"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/tvGSsoftware1" />

        <RadioGroup
            android:id="@+id/rgGSsoftware1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware2"
            app:layout_constraintLeft_toRightOf="@id/tvGSsoftware2"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware2">

            <RadioButton
                android:id="@+id/rbGSxpsp2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/rbGSxpsp2"
                android:textAlignment="center"
                android:textSize="10sp" />

            <RadioButton
                android:id="@+id/rbGSxpsp3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/rbGSxpsp3"
                android:textAlignment="center"
                android:textSize="10sp" />

        </RadioGroup>

        <TextView
            android:id="@+id/tvGSsoftware3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsTextsoftware3"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/tvGSsoftware2" />

        <EditText
            android:id="@+id/etGSsoftwareC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/etgsC"
            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware3"
            app:layout_constraintLeft_toRightOf="@id/tvGSsoftware3"
            app:layout_constraintRight_toLeftOf="@id/etGSsoftwareD"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware3" />


        <TextView
            android:id="@+id/tvGSsoftwareC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsGB"
            android:textAlignment="center"

            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware3"

            app:layout_constraintLeft_toRightOf="@id/etGSsoftwareC"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware3" />


        <EditText
            android:id="@+id/etGSsoftwareD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/etgsD"
            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware3"
            android:inputType="numberDecimal"
            app:layout_constraintLeft_toRightOf="@id/etGSsoftwareC"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware3" />


        <TextView
            android:id="@+id/tvGSsoftwareD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsGB"
            android:textAlignment="center"

            app:layout_constraintBottom_toBottomOf="@id/tvGSsoftware3"

            app:layout_constraintLeft_toRightOf="@id/etGSsoftwareD"
            app:layout_constraintTop_toTopOf="@id/tvGSsoftware3" />


        <TextView
            android:id="@+id/tvGScommon1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs24"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSsoftware3" />


        <EditText
            android:id="@+id/etGScommon24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="24"
            app:layout_constraintBottom_toBottomOf="@id/tvGScommon1"
            android:inputType="numberDecimal"
            app:layout_constraintLeft_toRightOf="@id/tvGScommon1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/tvGScommon1" />


        <TextView
            android:id="@+id/tvGScommon24V"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsV"
            android:textAlignment="center"

            app:layout_constraintBottom_toBottomOf="@id/tvGScommon1"

            app:layout_constraintLeft_toRightOf="@id/etGScommon24"
            app:layout_constraintTop_toTopOf="@id/tvGScommon1" />

        <TextView
            android:id="@+id/tvGScommon2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs8"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGScommon1" />


        <EditText
            android:id="@+id/etGScommon8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="8"
            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@id/tvGScommon2"
            app:layout_constraintLeft_toRightOf="@id/tvGScommon2"
            app:layout_constraintStart_toStartOf="@id/etGScommon24"

            app:layout_constraintTop_toTopOf="@id/tvGScommon2" />


        <TextView
            android:id="@+id/tvGScommon8V"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsV"
            android:textAlignment="center"

            app:layout_constraintBottom_toBottomOf="@id/tvGScommon2"

            app:layout_constraintLeft_toRightOf="@id/etGScommon8"
            app:layout_constraintTop_toTopOf="@id/tvGScommon2" />

        <TextView
            android:id="@+id/tvGScommon3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs12"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGScommon2" />


        <EditText
            android:id="@+id/etGScommon12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="12"

            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@id/tvGScommon3"
            app:layout_constraintLeft_toRightOf="@id/tvGScommon3"

            app:layout_constraintStart_toStartOf="@id/etGScommon24"
            app:layout_constraintTop_toTopOf="@id/tvGScommon3" />


        <TextView
            android:id="@+id/tvGScommon12V"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gsV"
            android:textAlignment="center"

            app:layout_constraintBottom_toBottomOf="@id/tvGScommon3"

            app:layout_constraintLeft_toRightOf="@id/etGScommon12"
            app:layout_constraintTop_toTopOf="@id/tvGScommon3" />


        <TextView
            android:id="@+id/tvGSincubator1_25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs1_25"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGScommon3" />


        <EditText
            android:id="@+id/etGSincubator1_25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="25"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSincubator1_25"
            app:layout_constraintLeft_toRightOf="@id/tvGSincubator1_25"
            app:layout_constraintTop_toTopOf="@id/tvGSincubator1_25" />

        <TextView
            android:id="@+id/tvGSincubator2_25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs2_25"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSincubator1_25" />


        <EditText
            android:id="@+id/etGSincubator2_25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="25"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSincubator2_25"
            app:layout_constraintLeft_toRightOf="@id/tvGSincubator2_25"
            app:layout_constraintTop_toTopOf="@id/tvGSincubator2_25" />

        <TextView
            android:id="@+id/tvGSincubator1_37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs1_37"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSincubator2_25" />


        <EditText
            android:id="@+id/etGSincubator1_37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="37"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSincubator1_37"
            app:layout_constraintLeft_toRightOf="@id/tvGSincubator1_37"
            app:layout_constraintTop_toTopOf="@id/tvGSincubator1_37" />

        <TextView
            android:id="@+id/tvGSincubator2_37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgs2_37"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSincubator1_37" />


        <EditText
            android:id="@+id/etGSincubator2_37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="37"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSincubator2_37"
            app:layout_constraintLeft_toRightOf="@id/tvGSincubator2_37"
            app:layout_constraintTop_toTopOf="@id/tvGSincubator2_37" />


        <TextView
            android:id="@+id/tvGScentrifugation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:labelFor="@id/tvGScentrifugation"
            android:text="@string/etgsCentrifugation"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSincubator2_37" />


        <EditText

            android:id="@+id/etGScentrifugation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGScentrifugation"
            app:layout_constraintLeft_toRightOf="@id/tvGScentrifugation"
            app:layout_constraintTop_toTopOf="@id/tvGScentrifugation" />

        <TextView
            android:id="@+id/tvGScentrifugationRPM"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgsRPM"
            android:textAlignment="center"
            app:layout_constraintLeft_toRightOf="@id/etGScentrifugation"
            app:layout_constraintBaseline_toBaselineOf="@id/etGScentrifugation"/>


        <TextView
            android:id="@+id/tvGSfluidVaccum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:labelFor="@id/etGSfluidVaccum"
            android:text="@string/tvGSfluidVaccum"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGScentrifugation" />


        <EditText

            android:id="@+id/etGSfluidVaccum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="600"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSfluidVaccum"
            app:layout_constraintLeft_toRightOf="@id/tvGSfluidVaccum"
            app:layout_constraintTop_toTopOf="@id/tvGSfluidVaccum" />

        <TextView
            android:id="@+id/tvGSfluidmBAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgsmBAR"
            android:textAlignment="center"
            app:layout_constraintLeft_toRightOf="@id/etGSfluidVaccum"
            app:layout_constraintBaseline_toBaselineOf="@id/etGSfluidVaccum" />

        <TextView
            android:id="@+id/tvGSfluidPressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:labelFor="@id/etGSfluidPressure"
            android:text="@string/tvGSfluidPressure"
            android:textAlignment="center"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/etGSfluidVaccum" />


        <EditText

            android:id="@+id/etGSfluidPressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="180"
            android:inputType="number"
            app:layout_constraintBottom_toBottomOf="@id/tvGSfluidPressure"
            app:layout_constraintLeft_toRightOf="@id/tvGSfluidPressure"
            app:layout_constraintTop_toTopOf="@id/tvGSfluidPressure" />

        <TextView
            android:id="@id/tvGSfluidmBAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/etgsmBAR"
            android:textAlignment="center"
            app:layout_constraintLeft_toRightOf="@id/etGSfluidPressure"
            app:layout_constraintBaseline_toBaselineOf="@id/etGSfluidPressure" />


        <!-- Parts -->
        <TextView
            android:id="@+id/tvGSparts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:text="@string/gsTextParts"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@id/tvGSfluidPressure" />


        <EditText
            android:id="@+id/partCount"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="@string/partCount"
            app:layout_constraintEnd_toStartOf="@id/partDescription"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvGSparts" />

        <EditText
            android:id="@+id/partDescription"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="@string/partDescription"
            app:layout_constraintEnd_toStartOf="@id/partSerial"

            app:layout_constraintHorizontal_weight="4"
            app:layout_constraintStart_toEndOf="@id/partCount"
            app:layout_constraintTop_toBottomOf="@id/tvGSparts" />

        <EditText
            android:id="@+id/partSerial"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="@string/partSerial"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="2"
            app:layout_constraintStart_toEndOf="@id/partDescription"
            app:layout_constraintTop_toBottomOf="@id/tvGSparts" />

        <View
            android:id="@+id/divider1"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintTop_toBottomOf="@id/partSerial" />
    </android.support.constraint.ConstraintLayout>
</ScrollView>